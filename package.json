{
  "name": "bluffmaster",
  "description": "Generate massive amounts of fake contextual data",
  "private": false,
  "version": "5.5.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/luciferreeves/bluffmaster.git"
  },
  "scripts": {
    "browser": "./node_modules/.bin/gulp browser",
    "jsdoc": "./node_modules/.bin/gulp jsdoc",
    "readme": "./node_modules/.bin/gulp readme",
    "lint": "node_modules/.bin/jshint ./lib --config ./.jshintrc",
    "test": "node_modules/.bin/mocha test/*.*.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\""
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "coveralls": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-mustache": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "ink-docstrap": "1.1.4",
    "jsdoc": "^3.4.0",
    "jshint": "0.9.0",
    "lodash": "^4.6.1",
    "mocha": "^8.1.1",
    "node-minify": "*",
    "nyc": "^15.1.0",
    "optimist": "0.3.5",
    "prettier": "^2.5.1",
    "sinon": "^9.0.3",
    "through2": "2.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vinyl-transform": "^1.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts"
}
