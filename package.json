{
  "name": "blaver",
  "description": "Generate massive amounts of fake contextual data",
  "private": false,
  "version": "6.6.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/luciferreeves/blaver.git"
  },
  "scripts": {
    "browser": "gulp browser",
    "jsdoc": "gulp jsdoc",
    "readme": "gulp readme",
    "lint": "eslint lib --fix",
    "test": "mocha test/*.*.js",
    "test:file": "mocha",
    "coverage": "nyc npm test",
    "coverage:file": "nyc mocha",
    "report": "nyc report --reporter=text-lcov",
    "cov:rep": "npm test && npm run report"
  },
  "nyc": {
    "all": false,
    "include": [
      "lib/**/*.js",
      "vendor/*.js"
    ],
    "exclude": [
      "coverage",
      "locales",
      "modules",
      "white",
      "docs",
      "docker",
      "public",
      "reports",
      "tests",
      "node_modules"
    ],
    "reporter": [
      "html",
      "lcov",
      "clover"
    ],
    "report-dir": "./reports/coverage"
  },
  "keywords":[
    "blaver",
    "blaver.js",
    "blaver.dev",
    "blaver-js",
    "faker",
    "faker.js",
    "faker-js",
    "faker-js-blaver",
    "marak",
    "priyansh",
    "reeves",
    "luciferreeves",
    "fake data generator",
    "fake data",
    "fake content generator",
    "fake contextual data generator",
    "fake contextual data"
  ],
  "devDependencies": {
    "browserify": "^16.5.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.6.0",
    "gulp": "^4.0.2",
    "gulp-gh-pages": "^0.5.4",
    "gulp-jsdoc3": "^3.0.0",
    "gulp-mustache": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "ink-docstrap": "1.1.4",
    "jsdoc": "^3.4.0",
    "lodash": "^4.6.1",
    "mocha": "^8.1.1",
    "node-minify": "*",
    "nyc": "^15.1.0",
    "optimist": "0.3.5",
    "shx": "^0.3.3",
    "sinon": "^9.0.3",
    "through2": "2.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vinyl-transform": "^1.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts"
}
